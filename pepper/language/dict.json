{"grammar": {
    "to be": {
      "does":{
        "tense": "present",
        "person":["he","she","it"]
      },
        "am": {
          "tense": "present",
          "person": "i"
        },
          "are":{
            "tense": "present",
            "person": ["you","we","they"]
          },
          "is":{
            "tense": "present",
            "person": ["he","she","it"]
          },
        "do": {
          "tense": "present"
        },
          "did":{
            "tense": "past"
          },
          "was":{
            "tense": "past",
            "person": ["i","he","she","it"]
          },
      "were": {
          "tense": "past",
          "person": ["we","you","they"]
        },
          "will": {
            "tense": "future"
          }

    },

  "pronouns": {
      "me":{
         "person": "first"
      },
        "i": {
          "person": "first",
          "number": "singular",
          "possessive": ["my", "mine"]
        },
          "you":{
            "person": "second",
          "number": ["singular","plural"],
          "possessive": ["your", "yours"]
          },
          "him":{
            "person":"third",
            "gender":"male"
          },
          "he":{
            "person": "third",
          "number": "singular",
            "gender": "male",
          "possessive": ["his"]
          },
        "she": {
          "person": "third",
          "number": "singular",
          "gender": "female",
          "possessive": ["her", "hers"]
        },
          "it":{
            "person": "third",
          "number": "neutral",
          "possessive": ["its"]
          },
      "we": {
          "person": "first",
          "number": "plural",
          "possessive": ["our","ours"]
        },
          "they":{
            "person": "third",
          "number": "plural",
          "possessive": ["their","theirs"]
          }

    },



  "question words": {
    "what": {
      "response": "object"
    },
    "who":{
      "response": "person"
    },
    "when":{
      "response": "time"
    },
    "where":{
      "response": "location"
    },
    "how": {
      "response": "manner"
    },
    "why":{
      "response": "reason"
    }
    },
  "verbs":
  {
    "like": {
      "sentiment":"positive",
      "past":"likes"
    },
    "dislike": {
      "sentiment":"negative"
    },
    "know":
    {
      "certainty": "high"
    },
    "think":
    {
      "certainty": "medium"
    },
    "meet":
    {
      "past":"met"
    }

  }
  }
}
